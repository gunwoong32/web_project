<!-- 패스워드1 입력, 패스워드2 입력해서
	두 패스워드가 일치하는지 판별해보세요 -->

<script type="text/javascript">

pw1 = prompt('패스워드1을 입력하세요')
pw2 = prompt('패스워드2를 입력하세요')



if (pw1 == pw2) {
	alert('패스워드가 일치합니다')
} else {
	alert('패스워드가 일치하지 않습니다.')

}





</script>


